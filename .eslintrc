{
  "extends": "airbnb",
  "globals": {
    "navigator": false,
    "window": false,
    "document": false,
    "registration": false,
    "fetch": false,
    "localStorage": false
  },
  "rules": {
    "react/jsx-filename-extension": false,
    "jsx-a11y/anchor-is-valid": false,
    "no-use-before-define": "off",
    "object-curly-newline": "off",
    "max-len": [2, 120]
  }
}
